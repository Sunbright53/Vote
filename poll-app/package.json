{
  "name": "poll-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pages:prep": "cp dist/index.html dist/404.html && touch dist/.nojekyll",
    "deploy": "npm run build && npm run pages:prep && cd .. && rm -rf docs && mv poll-app/dist docs && git add -A && git commit -m \"deploy\" || echo nochanges && git push && cd -"
  },
  "dependencies": {
    "chart.js": "^4.4.6",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.20"
  }
}
